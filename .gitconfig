[pull]
  rebase = true
[push]
  default = simple
  followTags = true
[user]
  name = Shrey Banga
  email = shrey@quip.com
[core]
  pager = diff-so-fancy | less --tabs=4 -RFX
[color "diff"]
  meta = yellow bold
  commit = green bold
  frag = green bold
  old = red bold
  new = green bold
  whitespace = red reverse
[color "diff-highlight"]
  oldNormal = red bold
  oldHighlight = "red bold reverse"
  newNormal = "green bold"
  newHighlight = "green bold reverse"
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[diff]
  tool = "code"
[merge]
  tool = "code"
[difftool "code"]
  cmd = code --wait --diff $LOCAL $REMOTE
[mergetool "code"]
  cmd = code --wait $MERGED
