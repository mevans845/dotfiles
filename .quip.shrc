export PATH=$PATH:$HOME/quip/bin

alias dd="docker-devel"
alias pi="docker-devel ipy"
alias pp="git pull && git push"
alias ghash='echo https://github.com/quip/quip/commit/$(git log -n 1 --pretty=format:"%h") | pbcopy'
alias ggo='open https://github.com/quip/quip/commit/$(git log -n 1 --pretty=format:"%h")'

export PROTOCOL_BUFFERS_PYTHON_IMPLEMENTATION="cpp"
export DISABLE_WV2=1
export DISABLE_MACROS=1
export BLAZE=1
ulimit -n 4096

cd quip
